{
  "name": "turahe/laravel-counters",
  "description": "management for counters in laravel system",
  "license": "MIT",
  "keywords": [
    "Turahe",
    "laravel",
    "counters",
    "increment",
    "decrement",
    "number of visitors",
    "visitors"
  ],
  "authors": [
    {
      "name": "Nur Wachid",
      "email": "wachid@outlook.com",
      "homepage": "https://www.wach.id"
    }
  ],
  "homepage": "https://github.com/turahe/laravel-counters",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^10.0 || ^11.0 || ^12.0"
  },
  "require-dev": {
    "laravel/pint": "^1.17",
    "orchestra/testbench": "^8.0 || ^9.0 || ^10.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Turahe\\Counters\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Turahe\\Counters\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "aliases": {
      "Counters": "Turahe\\Counters\\Facades\\Counters"
    },
    "laravel": {
      "providers": [
        "Turahe\\Counters\\CountersServiceProvider"
      ]
    }
  }
}
