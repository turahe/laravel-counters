{
  "name": "turahe/laravel-counters",
  "description": "management for counters in laravel system",
  "keywords": [
    "Turahe",
    "laravel",
    "counters",
    "increment",
    "decrement",
    "number of visitors",
    "visitors"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/console": "5.* || ^6.0 || ^7.0 || ^8.0",
    "illuminate/database": "5.* || ^6.0 || ^7.0 || ^8.0",
    "illuminate/support": "5.* || ^6.0 || ^7.0 || ^8.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "orchestra/testbench": "~3.8.4 || ^4.0 || ^5.0 || ^6.0",
    "phpspec/phpspec": "~5.1.1 || ^6.0 || ^7.0"
  },
  "prefer-stable": true,
  "authors": [
    {
      "name": "Nur Wachid",
      "email": "wachid@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Turahe\\Counters\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Turahe\\Counters\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Turahe\\Counters\\CountersServiceProvider"
      ]
    },
    "aliases": {
      "Counters": "Turahe\\Counters\\Facades\\Counters"
    }
  }
}
